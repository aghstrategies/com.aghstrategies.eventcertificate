<?xml version="1.0"?>
<extension key="com.aghstrategies.eventcertificate" type="module">
  <file>eventcertificate</file>
  <name>Event Certificate</name>
  <description>Creates a url to for users to visit to download their certificates</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>AGH Strategies</author>
    <email>alice@aghstrategies.com</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-12-27</releaseDate>
  <version>1.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Eventcertificate</namespace>
    <format>24.09.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
